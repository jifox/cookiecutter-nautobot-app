  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Nautobot (Remote)",
      "type": "python",
      "request": "attach",
      "connect": {
        "host": "127.0.0.1",
        "port": 6899
      },
      "pathMappings": [{
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/source"
      }],
      "django": true,
      "justMyCode": false
    },
    {
      "name": "Python: Worker (Remote)",
        "type": "python",
        "request": "attach",
      "connect": {
        "host": "127.0.0.1",
        "port": 6898
      },
      "pathMappings": [{
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/source"
      }],
      "django": true,
      "justMyCode": false
    }
  ]
}